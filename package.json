{
  "name": "tpyescript-next-redux-antd",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/juicyjusung/tpyescript-next-redux-antd",
  "author": "juicyjusung <juicyjusung@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next",
    "build:dev": "next build",
    "build": "cross-env NODE_ENV=production next build",
    "export": "next export",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "antd": "^3.24.3",
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "immer": "^4.0.2",
    "isomorphic-fetch": "^2.2.1",
    "next": "^9.1.1",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^4.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.9.1",
    "redux-saga": "^1.1.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^24.0.11",
    "@types/node": "^8.10.14",
    "@types/react": "^16.9.11",
    "@types/react-redux": "^7.1.5",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "@zeit/next-css": "^1.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-transform-define": "^1.3.0",
    "compression-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.7.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.4",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "jest.tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "enzyme.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/mocks.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/mocks.js"
    }
  }
}
